{
  "$schema": "https://json.schemastore.org/swcrc.json",
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript",
      "decorators": false,
      "dynamicImport": true
    },
    "transform": {
      "decoratorMetadata": false,
      "legacyDecorator": false
    },
    "keepClassNames": true,
    "externalHelpers": true,
    "loose": false
  },
  "module": {
    "type": "es6",
    "strict": true,
    "noInterop": true
  },
  "sourceMaps": true,
  "exclude": [
    "types/@types/postcss-advanced-variables/**/.*.ts$",
    "types/@types/postcss-modules-values/**/.*.ts$",
    "types/@types/postcss-env-function/**/.*.ts$",
    "packages/@types/shell-escape-tag/**/.*.ts$",
    "types/@types/postcss-extend-rule/**/.*.ts$",
    "packages/ergo-tagged-templates/**/.*.ts$",
    "types/@types/postcss-functions/**/.*.ts$",
    "types/@types/postcss-nesting/**/.*.ts$",
    "packages/starbeam-vitepress/**/.*.ts$",
    "types/@types/postcss-scss/**/.*.ts$",
    "types/@types/jsdoc-api/**/.*.ts$",
    "packages/wycats-utils/**/.*.ts$",
    "src/frameworks/react/**/.*.ts$",
    "packages/api-docs/**/.*.ts$",
    "packages/twoslash/**/.*.ts$",
    "packages/monaco/**/.*.ts$",
    "packages/omg/**/.*.ts$",
    "scripts/**/.*.ts$",
    "./**/.*.ts$",
    "node_modules/**/*.ts$"
  ]
}
